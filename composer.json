{
    "name": "geckoboom/whirlwind-application-testing",
    "description": "Functional testing for whirlwind micro framework",
    "type": "library",
    "require": {
        "php": "^8.0.0",
        "ext-json": "*",
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4",
        "justinrainbow/json-schema": "^5.2",
        "ext-mbstring": "*",
        "softcreatr/jsonpath": "^0.8.2"
    },
    "require-dev": {
        "whirlwind-framework/framework": "*",
        "squizlabs/php_codesniffer": "^3.6",
        "dg/bypass-finals": "^1.3",
        "whirlwind-framework/adapter-router-league": "^0.0.7"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WhirlwindApplicationTesting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WhirlwindApplicationTesting\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Artem Tsyganov",
            "email": "ontroip@gmail.com"
        }
    ]
}
